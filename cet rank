//cet rank//
#include <stdio.h>
int main()
{
    int rank;
    printf("enter the rank \n");
    scanf("%d",&rank);
    if(rank>22340)
    {
        printf("admission not possible in rnsit\n ");   
   }
   else 
   {
      if(rank<3250)
      {
        printf("he will get in all branches\n");
      }
      else 
       {
           printf("he will get in ec and mec branch \n");
       }
    } 
   return (0);    
}
