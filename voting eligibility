#include <stdio.h>
int main()
{
    int age,x;
    x=(age>18)?age:18;
    printf("enter the age a of person\n");
    scanf("%d",&age);
    if (age>18)
    {
        printf("person is eligeble for voting\n");
    }
    else
    {
        printf("person is not eligeble for voting\n");
        printf("the eligibility for  voting is 18 years\n");
    }
    return (0);
 }
